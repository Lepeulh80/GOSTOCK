{
  "version": "0.2.0",
  "configurations": [
    {
  "type": "node",
      "request": "launch",
      "name": "Next.js: Debug Development Server",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env.local",
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      }
    }
  ]
}
